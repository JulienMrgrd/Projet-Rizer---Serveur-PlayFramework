# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page


# Account
GET     /inscr/:login/:pass/:mail   	controllers.AccountRoutes.inscription(login:String,pass:String,mail:String)
GET     /connex/:login/:pass	    	controllers.AccountRoutes.connexion(login:String,pass:String)
GET     /check/:uuid			    	controllers.AccountRoutes.checkToken(uuid:String)
GET     /modifAcc/:uuid/:keys/:values controllers.AccountRoutes.modifyAccountInformations(uuid:String,keys:String,values:String)
GET     /visuaAcc/:uuid/:idAcc    		controllers.AccountRoutes.visualizeAccountInformations(uuid:String,idAcc:Integer)
GET     /deleteAcc/:uuid			    controllers.AccountRoutes.deleteAccount(uuid:String)
GET     /becomeArt/:uuid   				controllers.AccountRoutes.sendDemandBecomeArtist(uuid:String)
